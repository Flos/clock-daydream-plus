<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!--
     Entries listed in the ListPreference when invoking the clock style
     preference.
-->
    <string-array name="clock_size_entries">
        <item>Small</item>
        <item>Medium</item>
        <item>Large</item>
        <item>X-Large</item>
        <item>2X-Large</item>
    </string-array>

    <!--
         Values that are retrieved from the ListPreference. These must match
         the clock_style_entries above.
    -->
    <string-array name="clock_size_values" translatable="false">
        <item>small</item>
        <item>medium</item>
        <item>large</item>
        <item>xlarge</item>
        <item>2xarge</item>
    </string-array>

    <!-- Default clock size. -->
    <string name="default_clock_size">large</string>
</resources>